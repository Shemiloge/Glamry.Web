@model Glamry.Web.Models.RegistrationModel

<div class="container">
    <div class="form-group row">
        
    </div>
    <div class="form-group row">
      <div class="col-lg-5 ">
            @Html.LabelFor(x => x.ImageUpload)
        </div>
        <div class="col-lg-5">
            <div class="col-lg-2" ></div>
            <input name="image" id="image" type="file" />
            @Html.ValidationMessageFor(x => x.ImageUpload)
        </div>
    </div>
    <div class="form-group row container">
        <button type="submit" value="Upload" class="success">Upload</button>
    </div>
</div>
